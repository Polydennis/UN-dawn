          "name": "Foto-Upload",
          "settings": {
            "custom_liquid": "<style>\n  .custom-file-upload-container {\n    margin-bottom: 2rem;\n  }\n  \n  .input-hidden {\n    opacity: 0;\n    width: 0.1px;\n    height: 0.1px;\n    position: absolute;\n    z-index: -1;\n    overflow: hidden;\n  }\n\n  .custom-upload-label {\n    width: 100%;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    cursor: pointer;\n  }\n\n  .custom-upload-label svg {\n    width: 2rem;\n    height: 2rem;\n    margin-right: 1rem;\n    fill: currentColor;\n  }\n\n  .file-name-text {\n    display: block;\n    margin-top: 1rem;\n    font-size: 1.3rem;\n    color: rgba(var(--color-foreground), 0.75);\n    font-style: italic;\n  }\n\n  .upload-preview-wrapper {\n    margin-top: 1.5rem;\n    display: none;\n    border: 1px solid rgba(var(--color-foreground), 0.08);\n    padding: 1.5rem;\n    border-radius: var(--inputs-radius-outset);\n  }\n  \n  .upload-preview-wrapper.is-visible {\n    display: block;\n  }\n\n  .upload-preview-img {\n    max-width: 150px;\n    height: auto;\n    display: block;\n    margin-bottom: 1rem;\n    border-radius: 0.4rem;\n    box-shadow: 0 4px 6px rgba(0,0,0,0.1);\n  }\n\n  .remove-file-btn {\n    background: none;\n    border: none;\n    text-decoration: underline;\n    text-underline-offset: 0.3rem;\n    color: rgba(var(--color-foreground), 0.75);\n    cursor: pointer;\n    font-size: 1.2rem;\n    padding: 0;\n  }\n</style>\n\n<div class=\"custom-file-upload-container\">\n  \n  <input \n    type=\"file\" \n    id=\"file-upload-{{ section.id }}\" \n    class=\"input-hidden\"\n    name=\"properties[Photo]\" \n    form=\"product-form-{{ section.id }}\"\n    accept=\"image/png, image/jpeg, image/jpg, image/gif, image/webp\"\n  >\n\n  <label for=\"file-upload-{{ section.id }}\" class=\"button button--tertiary custom-upload-label\">\n    <svg viewBox=\"0 0 24 24\" xmlns=\"http://www.w3.org/2000/svg\"><path d=\"M20 5h-3.17L15 3H9L7.17 5H4c-1.1 0-2 .9-2 2v12c0 1.1.9 2 2 2h16c1.1 0 2-.9 2-2V7c0-1.1-.9-2-2-2zm0 14H4V7h4.05l1.83-2h4.24l1.83 2H20v12z\"/><path d=\"M12 9c-2.21 0-4 1.79-4 4s1.79 4 4 4 4-1.79 4-4-1.79-4-4-4zm0 6c-1.1 0-2-.9-2-2s.9-2 2-2 2 .9 2 2-.9 2-2 2z\"/></svg>\n    {{ 'products.product.upload_photo' | t }}\n  </label>\n  \n  <span id=\"file-name-{{ section.id }}\" class=\"file-name-text\"></span>\n\n  <div id=\"preview-wrapper-{{ section.id }}\" class=\"upload-preview-wrapper\">\n    <img id=\"preview-img-{{ section.id }}\" class=\"upload-preview-img\" src=\"\" alt=\"\">\n    <button type=\"button\" id=\"remove-btn-{{ section.id }}\" class=\"remove-file-btn\">\n      {{ 'cart.general.remove' | t | default: 'Remove' }}\n    </button>\n  </div>\n\n</div>\n\n<script>\n  document.addEventListener(\"DOMContentLoaded\", function() {\n    var productForm = document.getElementById('product-form-{{ section.id }}');\n    if (productForm) {\n      productForm.setAttribute('enctype', 'multipart/form-data');\n    }\n\n    const fileInput = document.getElementById('file-upload-{{ section.id }}');\n    const fileNameDisplay = document.getElementById('file-name-{{ section.id }}');\n    const previewWrapper = document.getElementById('preview-wrapper-{{ section.id }}');\n    const previewImg = document.getElementById('preview-img-{{ section.id }}');\n    const removeBtn = document.getElementById('remove-btn-{{ section.id }}');\n\n    if (fileInput) {\n      fileInput.addEventListener('change', function(event) {\n        const file = event.target.files[0];\n\n        if (file) {\n          fileNameDisplay.textContent = file.name;\n\n          const reader = new FileReader();\n          reader.onload = function(e) {\n            previewImg.src = e.target.result;\n            previewWrapper.classList.add('is-visible');\n          }\n          reader.readAsDataURL(file);\n        } else {\n          removeImage();\n        }\n      });\n    }\n\n    if (removeBtn) {\n      removeBtn.addEventListener('click', removeImage);\n    }\n\n    function removeImage() {\n      fileInput.value = ''; \n      fileNameDisplay.textContent = '';\n      previewImg.src = '';\n      previewWrapper.classList.remove('is-visible');\n    }\n  });\n</script>"
